from os import TextFile

def part_1(lines: [string]) -> i64:
    return 0

def day_06():
    lines: [string] = []

    for line in TextFile(f"{__assets__}/day_06.txt").read().strip().split("\n"):
        mo = line.match(re"(\d+)")
        lines.append(mo.group(1))

    print(part_1(lines))

@test
def test_all():
    lines = [
        ""
    ]
    assert part_1(lines) == 0
